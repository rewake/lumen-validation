{
  "name": "rewake/lumen-validation",
  "type": "library",
  "description": "Enhanced validation for Lumen",
  "keywords": ["validation", "lumen"],
  "homepage": "http://github.com",
  "license": "proprietary",
  "authors": [
    {
      "name": "Richard Komatz",
      "email": "r.komatz@gmail.com"
    }
  ],
  "require": {
    "illuminate/validation": "^5.2",
    "illuminate/translation": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Rewake\\": "src/Rewake"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.1.2-dev"
    }
  },
  "scripts": {
    "post-package-install": [
      "Rewake\\Lumen\\ComposerScripts\\CreateDefaultValidationLangFile::postPackageInstall"
    ]
  },
  "require-dev": {
    "composer/composer": "^1.6"
  }
}
